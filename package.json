{
  "name": "fukuro-design-system",
  "version": "0.0.1",
  "description": "Fukuro Design System using Web Components",
  "main": "src/index.js",
  "scripts": {
    "build": "rimraf dist && npm run build:documentation && npm run build:www && npm run build:esnext && npm run build:storybook",
    "build:documentation": "wca analyze src --format json --outFile src/custom-elements.json",
    "build:storybook": "build-storybook -o ./dist/storybook",
    "build:www": "webpack --progress --config ./webpack/webpack.config.www",
    "build:esnext": "webpack --progress --config ./webpack/webpack.config.esnext",
    "start:storybook": "npm run build:documentation && start-storybook -p 6006",
    "start:webpack": "webpack-dev-server --progress --config ./webpack/webpack.config.dev.js",
    "start:tswatch": "tsc --watch"
  },
  "author": "Fukurosan",
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.6.4",
    "lit-element": "^2.2.1",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/addon-backgrounds": "^5.3.12",
    "@storybook/addon-docs": "^5.3.12",
    "@storybook/addon-knobs": "^5.3.12",
    "@storybook/addon-viewport": "^5.3.12",
    "@storybook/web-components": "^5.3.12",
    "@ungap/custom-elements-builtin": "^0.2.10",
    "@webcomponents/webcomponentsjs": "^2.4.1",
    "@whitespace/storybook-addon-html": "^1.2.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.1",
    "raw-loader": "^3.1.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.0",
    "storybook-addon-web-components-knobs": "^0.2.7",
    "typescript": "^3.7.5",
    "web-component-analyzer": "^1.0.2",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.2"
  }
}
