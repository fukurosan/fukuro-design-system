{
  "name": "fukuro-design-system",
  "version": "0.0.1",
  "description": "Fukuro Design System using Web Components",
  "main": "./dist/es2017/ouswc-bundle.js",
  "module": "./dist/esm/ouswc-bundle.mjs",
  "unpkg": "./dist/www/ouswc-bundle.js",
  "jsdelivr": "./dist/www/ouswc-bundle.js",
  "scripts": {
    "test": "jest --no-cache --verbose",
    "prettier": "prettier \"*/**/*.js\" --write",
    "lint": "eslint \"*/**/*.js\"",
    "lint:fix": "npm run prettier && eslint \"*/**/*.js\" --fix",
    "build": "npm run lint:fix && npm run build:documentation && rimraf dist && npm run rollup && npm run test && npm run build:storybook",
    "build:documentation": "wca analyze src --format json --outFile src/custom-elements.json",
    "build:storybook": "build-storybook -o ./dist/storybook",
    "build:www": "npx rollup -c --environment WWW",
    "build:esnext": "npx rollup -c --environment ESNEXT",
    "build:esm": "npx rollup -c --environment ESM",
    "rollup": "npx rollup -c --environment WWW,ESNEXT,ESM",
    "start:storybook": "npm run build:documentation && start-storybook -p 6006",
    "start:rollup": "npx rollup -c rollup.config.dev.js --watch",
    "start:tswatch": "tsc --watch",
    "start": "npx rollup -c rollup.config.dev.js --watch"
  },
  "author": "Fukurosan",
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.2.1",
    "lit-element": "^2.2.1",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-typescript": "^7.8.3",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@storybook/addon-backgrounds": "^5.3.12",
    "@storybook/addon-docs": "^5.3.12",
    "@storybook/addon-knobs": "^5.3.12",
    "@storybook/addon-viewport": "^5.3.12",
    "@storybook/web-components": "^5.3.12",
    "@ungap/custom-elements-builtin": "^0.2.10",
    "@webcomponents/webcomponentsjs": "^2.4.1",
    "@whitespace/storybook-addon-html": "^1.2.1",
    "acorn": "^6.4.2",
    "babel-loader": "^8.0.6",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-image-snapshot": "^4.3.0",
    "lint-staged": "^10.5.4",
    "node-sass": "^4.14.1",
    "prettier": "^2.2.1",
    "puppeteer": "^7.0.1",
    "raw-loader": "^3.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.5",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass-loader": "^8.0.0",
    "storybook-addon-web-components-knobs": "^0.2.7",
    "typescript": "^3.7.5",
    "web-component-analyzer": "^1.0.2",
    "webpack": "^4.35.0"
  }
}
